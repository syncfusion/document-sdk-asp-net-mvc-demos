@using Syncfusion.EJ2

@section ActionDescription{
    This sample demonstrates cloning an individual slide from a PowerPoint presentation and merge it to another PowerPoint presentation with source and destination paste options.
}

@section Description{
    <b>Features:</b>
    <br />
    <br />
    <ul>
        <li>Copy an individual PowerPoint slide and paste it to the same or different presentation.</li>
        <li>Slide elements like text, shapes, charts, tables, SmartArt diagrams and images etc., can be copied and moved between PowerPoint presentations.</li>
        <li>Copy the slides in a section and paste those to another section or PowerPoint presentation.</li>
    </ul>
    <br />
}
@section ControlsSection{
    <div class="control-section">
        @{ViewData["Title"] = "    Essential Presentation (Presentation) : CloningSlides : Syncfusion";}

        @{Html.BeginForm("CloningSlides", "PowerPoint", FormMethod.Post);
            {
                <div class="tablediv">
                    <div class="rowdiv">
                        <text>
                            Click the button to view a presentation document generated by Essential Presentation. Please note that Microsoft PowerPoint viewer is required to view the resultant document.
                        </text>
                    </div>
                    <br />
                    <div class="rowdiv">
                        <div class="celldiv">
                            <input type="radio" id="Radio3" name="Group1" value="DestinationTheme" checked="checked" />
                            <label for="Radio3">
                                Use Destination Theme
                            </label>
                            <input type="radio" id="Radio4" name="Group1" style="margin-left: 7px" value="SourceFormatting" />
                            <label for="Radio4">
                                Keep Source Formatting
                            </label>
                        </div>
                    </div>
                    <br />
                    <div class="rowdiv">
                        <div class="celldiv">
                            <input type="submit" class="e-btn" name="button" value="Generate Document" style="width:200px;" />
                        </div>
                        <br />
                    </div>
                </div>
                Html.EndForm();
            }
        }
    </div>
}
@section Title{
    <title>ASP.NET MVC PowerPoint (Presentation) library - Cloning Slide Example - Syncfusion Demos </title>
}
@section Meta{
    <meta name="description" content="This example demonstrates the Cloning Slide in ASP.NET MVC PowerPoint (Presentation) library. Explore here for more details." />
}
@section Header{
    <h1 class='sb-sample-text'>Example of Cloning Slide in ASP.NET MVC PowerPoint (Presentation) Library</h1>
}